<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Inmoviliaria El Trebol los mejores inmuebles." />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/bootstrap-theme")
    @Styles.Render("~/css/fontAwesome")
    <link href='https://fonts.googleapis.com/css?family=Allerta+Stencil|Michroma|Patua+One' rel='stylesheet' type='text/css'>
    @Styles.Render("~/Content/admin")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="container-fluid">
        @Html.Partial("~/Views/Shared/Back_end/Headers/Header_admin.cshtml")
        <div class="row">
            <div id="menu_lateral_admin" class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
                @RenderPage("~/Views/Shared/Back_end/Menus/menu_lateral_back.cshtml")
            </div>
            @RenderBody()
            <div id="notificaciones_admin" class="col-xs-12 col-sm-12 col-md-2 col-lg-2"></div>
        </div>
        @Html.Partial("~/Views/Shared/Back_end/Footers/Footer_admin.cshtml")
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/script/bootstrap")
    @RenderSection("scripts_admin", required: false)
</body>
</html>